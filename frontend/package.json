    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@stomp/stompjs": "^7.0.0",
    "axios": "^1.6.7",
    "bulma": "^1.0.0",
    "chalk": "^5.3.0",
    "jwt-decode": "^4.0.0",
    "loglevel": "^1.9.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "plausible-tracker": "^0.3.8",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
