    "@vue/tsconfig": "^0.8.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "^10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.93.2",
