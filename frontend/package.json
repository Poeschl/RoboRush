    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@neoconfetti/vue": "^2.2.1",
    "@stomp/stompjs": "^7.1.1",
    "axios": "^1.12.0",
    "bulma": "^1.0.4",
    "chalk": "^5.4.1",
    "jwt-decode": "^4.0.0",
    "loglevel": "^1.9.2",
