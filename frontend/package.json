    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.71.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.3"
  }
